<?xml version="1.0" encoding="utf-8"?>
<xs:schema
    targetNamespace="uz:rwc:document:common:1.11"
    attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    xmlns:tns="uz:rwc:document:common:1.11"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:common="uz:rwc:document:common:1.11"
    >

  <xs:complexType name='changes'>
    <xs:attribute name='target' type='xs:string' />
  </xs:complexType>

  <xs:complexType name='pereadr_info' >
    <xs:attribute name='date_nakaz' type='common:Date' >
      <xs:annotation>
        <xs:documentation>Дата надання наказу на переадресування вантажу</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='date_pereadr' type='common:Date' >
      <xs:annotation>
        <xs:documentation>Дата здійснення операції переадресування за первинним перевізним документом</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='distance' type='common:N_5' >
      <xs:annotation>
        <xs:documentation>Відстань від станції переадресування до станції призначення (у км)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='nakaz' type='common:C_100' >
      <xs:annotation>
        <xs:documentation>Номер наказу на переадресування вантажу</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='nom_perv_doc' type='common:N_8' >
      <xs:annotation>
        <xs:documentation>Номер первинного перевізного документа</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='ser_perv_doc' type='common:C_4' >
      <xs:annotation>
        <xs:documentation>Серія первинного перевізного документа</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='type_pay_old' type='common:PayKind' >
      <xs:annotation>
        <xs:documentation>Вид розрахунку у перевізному документі до виконання переадресування</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:attributeGroup name='route-attributes' >
    <xs:attribute name='stn_from' type='common:EsrCode' use='required' >
      <xs:annotation>
        <xs:documentation>Код станції відправлення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='name_from' type='common:C_50' use='required' >
      <xs:annotation>
        <xs:documentation>Назва станції відправлення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='rw_from' type='common:N_2' use='required' >
      <xs:annotation>
        <xs:documentation>код залізниці відправлення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='stn_to' type='common:EsrCode' use='required' >
      <xs:annotation>
        <xs:documentation>Код станції призначення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='name_to' type='common:C_50' use='required' >
      <xs:annotation>
        <xs:documentation>Назва станції призначення </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='rw_to' type='common:N_2' use='required'>
      <xs:annotation>
        <xs:documentation>Код залізниці призначення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='zd_otpr' type='common:N_4' use='optional'>
      <xs:annotation>
        <xs:documentation>Код залізниці відправлення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='zd_nazn' type='common:N_4' use='optional'>
      <xs:annotation>
        <xs:documentation>Код залізниці призначення</xs:documentation>
      </xs:annotation>
    </xs:attribute>

    <xs:attribute name='index' type='common:N_2' use='required' >
      <xs:annotation>
        <xs:documentation>Порядковий номер маршруту прямування (нумерація ведеться з 0). Нові маршрути прямування додаються при переадресуваннях або при досиланнях вантажу.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>

  <xs:complexType name='base-route'>
    <xs:attributeGroup ref='common:route-attributes' />
  </xs:complexType>

  <xs:complexType name='base-route-for-changes'>
    <xs:attribute name='stn_from' type='common:EsrCode' use='optional' />
    <xs:attribute name='name_from' type='common:C_50' use='optional' />
    <xs:attribute name='rw_from' type='common:N_2' use='optional' />
    <xs:attribute name='stn_to' type='common:EsrCode' use='optional' />
    <xs:attribute name='name_to' type='common:C_50' use='optional' />
    <xs:attribute name='rw_to' type='common:N_2' use='optional' />
    <xs:attribute name='zd_otpr' type='common:N_4' use='optional' />
    <xs:attribute name='zd_nazn' type='common:N_4' use='optional' />
    <xs:attribute name='index' type='common:N_2' use='optional' />
  </xs:complexType>

  <xs:complexType name='route'>
    <xs:complexContent>
      <xs:extension base='common:base-route'>
        <xs:sequence>
          <xs:element name='JOINT' type='common:joint' minOccurs='0' maxOccurs='100' />
          <xs:element name='PEREADR_INFO' type='common:pereadr_info' minOccurs='0' maxOccurs='1' />
          <xs:element name='ROUTE_LOC' type='common:route-loc' minOccurs='0' maxOccurs='1' />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name='route-loc'>
    <xs:attribute name='lang' type='common:CimLang' use='required' >
      <xs:annotation>
        <xs:documentation>Двозначний код другої мови перевізного документа по стандарту ІSO 639.1 (для ЦІМ та ЦІМ/СМГС бланка)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='name_from' type='common:C_50' use='required' >
      <xs:annotation>
        <xs:documentation>Код залізниці відправлення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='name_to' type='common:C_50' use='required' >
      <xs:annotation>
        <xs:documentation>Код залізниці призначення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name='route-loc-for-changes'>
    <xs:attribute name='lang' type='common:CimLang' use='optional' />
    <xs:attribute name='name_from' type='common:C_50' use='optional' />
    <xs:attribute name='name_to' type='common:C_50' use='optional' />
  </xs:complexType>

  <xs:complexType name='text'>
    <xs:attribute name='carload_agent_name' type='common:C_120' >
      <xs:annotation>
        <xs:documentation>Представитель вагонной службы </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='marks' type='common:C_400' >
      <xs:annotation>
        <xs:documentation>Відмітки не обов'язкові для залізниці </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='without_client' type='common:N_1' >
      <xs:annotation>
        <xs:documentation>Признак без зазначення клієнта</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='delivery_receipt' type='common:C_300' >
      <xs:annotation>
        <xs:documentation>Расписка о получении</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='prescript' type='common:C_50' >
      <xs:annotation>
        <xs:documentation>Розпорядження №</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='prescript_date' type='common:DateTime' >
      <xs:annotation>
        <xs:documentation>Дата розпорядження</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='zayava' type='common:C_800' >
      <xs:annotation>
        <xs:documentation>Особливі відмітки відправника</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='name_gr' type='common:C_800' >
      <xs:annotation>
        <xs:documentation>Додаткові відомості про вантаж</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='nom_dog' type='common:C_30' >
      <xs:annotation>
        <xs:documentation>Номер договору експорту/імпорту</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='nom_dog_sender_rw' type='common:C_30' >
      <xs:annotation>
        <xs:documentation>Номер договору між відправником та залізницею</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='outside_payer_code' type='common:N_8' >
      <xs:annotation>
        <xs:documentation>Код платника за межами УЗ</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='branch' type='common:C_35' >
      <xs:annotation>
        <xs:documentation>Найменування власника під'їзної колії </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='calc_reason' type='common:C_100' >
      <xs:annotation>
        <xs:documentation>Підстави для розрахунку</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='sing' type='common:C_200' >
      <xs:annotation>
        <xs:documentation>Знаки  </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='marks_gr' type='common:C_200' >
      <xs:annotation>
        <xs:documentation>Марки</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='nom_gr' type='common:C_200' >
      <xs:annotation>
        <xs:documentation>Номери</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='mount_para' type='common:C_30' >
      <xs:annotation>
        <xs:documentation>Параграфи ТУ , згідно яких закріплений вантаж</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='mount_chapter' type='common:C_30' >
      <xs:annotation>
        <xs:documentation>Розділи ТУ , згідно яких закріплений вантаж</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='carrier_from' type='common:N_4' >
      <xs:annotation>
        <xs:documentation>Компанія-перевізник при відправленні з країн ЦІМ</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='create_place' type='common:C_120' >
      <xs:annotation>
        <xs:documentation>Місце оформлення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='create_place_de' type='common:C_120' >
      <xs:annotation>
        <xs:documentation>Місце оформлення на другій мові документа (німецька або англійська для ЦІМ та ЦІМ/СМГС бланка)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='date_permit' type='common:Date' >
      <xs:annotation>
        <xs:documentation>Дата надання дозволу/довіреності на право використання власного вагона</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='trans_plan' type='common:NC_10' >
      <xs:annotation>
        <xs:documentation>Номер транспортного плану</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='nom_permit' type='common:C_10' >
      <xs:annotation>
        <xs:documentation>Номер дозволу/довіреності на право використання власного вагона</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='loader_position' type='common:C_50' >
      <xs:annotation>
        <xs:documentation>Посада особи, відповідальної за навантаження</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='loader_fio' type='common:C_50' >
      <xs:annotation>
        <xs:documentation>Прізвище та ім'я по батькові особи, відповідальної за навантаження</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='rwc_doc_name' type='common:C_200'>
      <xs:annotation>
        <xs:documentation>Логічне ім'я документа</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='stn_accum' type='common:EsrCode' use='optional'>
      <xs:annotation>
        <xs:documentation>Код станції накопичення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='stn_name_accum' type='common:C_50' use='optional'>
      <xs:annotation>
        <xs:documentation>Найменування станції накопичення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name='pac'>
    <xs:attribute name='kol_pac' type='common:N_7' >
      <xs:annotation>
        <xs:documentation>Кількість  місць упаковки</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='weight_place_br' type='common:N_5' >
      <xs:annotation>
        <xs:documentation>Стандартна маса одного місця брутто (кг)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='weight_place_net' type='common:N_5' >
      <xs:annotation>
        <xs:documentation>Стандартна маса одного місця нетто (кг)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name='taks'>
    <xs:attribute name='coeff_type' type='common:CoeffKind' >
      <xs:annotation>
        <xs:documentation>Вид коефіцієнта </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='coeff_value' type='common:N_6_4' >
      <xs:annotation>
        <xs:documentation>Значення коефіцієнта</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='coeff_mode' type='common:CoeffModeKind' >
      <xs:annotation>
        <xs:documentation>Ознака введення значення коефіцієнта</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='iskl_tar' type='common:N_4' >
      <xs:annotation>
        <xs:documentation>Винятковий  тариф</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='iskl_mode' type='common:ValueKind' >
      <xs:annotation>
        <xs:documentation>Ознака введення виняткового тарифу </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name='schema'>
    <xs:attribute name='carry_sch' type='common:R_2_1' >
      <xs:annotation>
        <xs:documentation>Схема таксування по відправленню</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='pr_sch' type='common:ValueKind' >
      <xs:annotation>
        <xs:documentation>Ознака виставлення схеми таксування</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:attributeGroup name='joint-attributes'>
    <xs:attribute name='cross_time' type='common:Date' >
      <xs:annotation>
        <xs:documentation>Дата та час перетину кордону</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='direction' type='common:JointDirection' >
      <xs:annotation>
        <xs:documentation>Напрямок при перетині кордону</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='admin' type='common:N_2' >
      <xs:annotation>
        <xs:documentation>Код адміністрації станції перетину кордону</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='port_name' type='common:C_150' >
      <xs:annotation>
        <xs:documentation>Найменування порту перехідного стикового пункту</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='stn' type='common:EsrCode' >
      <xs:annotation>
        <xs:documentation>Код станції перетину кордону</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='stn_name' type='common:C_50' >
      <xs:annotation>
        <xs:documentation>Найменування станції перетину кордону</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='zd_kod' type='common:N_4'>
      <xs:annotation>
        <xs:documentation>Код залізниці прикордонної станції</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>

  <xs:complexType name='joint-loc'>
    <xs:attribute name='lang' use='required' type='common:CimLang' >
      <xs:annotation>
        <xs:documentation>Двозначний код другої мови перевізного документа по стандарту ІSO 639.1 (для ЦІМ та ЦІМ/СМГС бланка)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='stn_name' type='common:C_100' >
      <xs:annotation>
        <xs:documentation>Найменування станції перетину кордону на другій мові перевізного документа (для ЦІМ та ЦІМ/СМГС бланка)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:complexType name='joint-loc-for-changes'>
    <xs:attribute name='lang' type='common:CimLang'   use='optional' />
    <xs:attribute name='stn_name' type='common:C_100' use='optional' />
  </xs:complexType>

  <xs:complexType name='base-joint'>
    <xs:attributeGroup ref='common:joint-attributes' />
  </xs:complexType>

  <xs:complexType name='joint'>
    <xs:complexContent>
      <xs:extension base='common:base-joint'>
        <xs:sequence >
          <xs:element name='JOINT_LOC' type='common:joint-loc' minOccurs='0' maxOccurs='1' />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name='zpu'>
    <xs:attribute name='nom_zpu' type='common:C_20' >
      <xs:annotation>
        <xs:documentation>Номер ЗПП</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='esr_zpu' type='common:EsrCode' >
      <xs:annotation>
        <xs:documentation>Код станції накладання пломби або ЗПП</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='sobst_zpu' type='common:SealOwnerKind' >
      <xs:annotation>
        <xs:documentation>Ознака  власності пломб або ЗПП</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='zpu' type='common:SealKind' >
      <xs:annotation>
        <xs:documentation>Код виду ЗПП </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='zd_kod' type='common:N_4' >
      <xs:annotation>
        <xs:documentation>Код залізниці накладання пломби</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>

  <xs:attributeGroup name='payer-attributes'>
    <xs:attribute name='kod_adm' type='common:N_2' >
      <xs:annotation>
        <xs:documentation>Код адміністрації платника (тільки для транзитних платників)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='carrier_kod' type='common:N_4' >
      <xs:annotation>
        <xs:documentation>Код перевізника</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='carrier_name' type='common:C_50' >
      <xs:annotation>
        <xs:documentation>Скорочене найменування перевізника</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='currency' type='common:N_3' >
      <xs:annotation>
        <xs:documentation>Бажана валюта сплати транзитного платника</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='date_carrier_dog' type='common:Date' >
      <xs:annotation>
        <xs:documentation>Дата укладення договору між перевізником та платником за перевезення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='kod_plat' type='common:C_20' >
      <xs:annotation>
        <xs:documentation>Код платника (для платника по відправленню/прибуттю — код з 7 цифр,  для транзитного платника по УЗ — 10 цифр, інші — до 20 символів)</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='name_plat' type='common:C_300' >
      <xs:annotation>
        <xs:documentation>Найменування платника</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='num_carrier_dog' type='common:C_50' >
      <xs:annotation>
        <xs:documentation>Номер договору укладеного між перевізником та платником за перевезення</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='podkod_exp' type='common:N_7' >
      <xs:annotation>
        <xs:documentation>Підкод платника (експедитора) </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='sector_num' type='common:N_2' >
      <xs:annotation>
        <xs:documentation>Номер ділянки</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='type' type='common:PayerKind' >
      <xs:annotation>
        <xs:documentation>Тип платника</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>

  <xs:complexType name='consignment-pay'>
    <xs:complexContent>
      <xs:extension base='common:pay'>
        <xs:attribute name='ser_doc' type='common:C_4' >
          <xs:annotation>
            <xs:documentation>Серія документу </xs:documentation>
