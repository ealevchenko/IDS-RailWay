@using WEB_UI.App_LocalResources;
@{
    ViewBag.Title = @IDSRWTResource.wsd_title;
}

@section styles {
    @Styles.Render("~/DataTables/css")
    @Styles.Render("~/DateTime/css")
    <link rel="stylesheet" type="text/css" href="~/Content/view/idsrwt/wsd.css">
    @*Подключим модули*@
    <link href="~/Content/module/cblist_station.css" rel="stylesheet" />
    <link href="~/Content/module/table_tree_way.css" rel="stylesheet" />
    <link href="~/Content/module/wsd/table_cars_operation.css" rel="stylesheet" /><!-- CSS стили модуля "Таблица вагонов по операциям детально" -->
    <link href="~/Content/module/wsd/operation_send.css" rel="stylesheet" /><!-- CSS стили модуля "Операции отправки вагонов на станции АМКР" -->


}

@section scripts {
    @Scripts.Render("~/bundles/Moment")
    @Scripts.Render("~/bundles/DataTables")
    @Scripts.Render("~/bundles/DateTime")

    <script src="~/Scripts/view/shared/common.js"></script> <!-- Общая библиотека методов программы -->
    <script src="~/Scripts/api/shared.js"></script> <!-- Общая библиотека методов API -->
    <script src="~/Scripts/api/ids_global.js"></script> <!-- Общая библиотека методов API -->
    <script src="~/Scripts/api/ids_sap.js"></script> <!-- Class API SAP функции для работы с САП -->
    <script src="~/Scripts/api/uz_directory.js"></script> <!-- Class API УЗ Справочники -->
    <script src="~/Scripts/api/ids_directory.js"></script> <!-- Class API ИДС Справочники -->
    <script src="~/Scripts/api/metrans.js"></script> <!-- Class API МЕТРАНС -->
    <script src="~/Scripts/api/ids_rwt.js"></script> <!-- Class API ИДС Прием вагонов -->
    <script src="~/Scripts/api/ids_transfer.js"></script> <!-- Class API ИДС Переносы -->
    <script src="~/Scripts/api/uz_sms.js"></script> <!-- Class API Работа с модулем согласования -->

    <script src="~/Scripts/api/ids_direct.js"></script><!-- Новая библиотека-модуль справочники ИДС -->
    <script src="~/Scripts/api/ids_wsd.js"></script><!-- Новая библиотека-модуль для работы с данными WSD -->
    @*================Подключим модули=============================================*@
    <script src="~/Scripts/module/shared/common.js"></script>               <!-- Модуль Общих класов (для работы с формами и HTML) -->
    <script src="~/Scripts/module/shared/modal_confirm_form.js"></script>   <!-- Модуль "Форма сообщений подтверждения" -->
    <script src="~/Scripts/module/cblist_station.js"></script>
    <script src="~/Scripts/module/table_tree_way.js"></script>
    <script src="~/Scripts/module/wsd/table_cars_operation.js"></script>    <!-- Модуль "Таблица вагонов по операциям детально" -->
    <script src="~/Scripts/module/wsd/operation_send.js"></script>          <!-- Модуль "Операции отправки вагонов на станции АМКР" -->

    <script src="~/Scripts/plugins/pl_dialog_confirm.js"></script> <!-- Плагин -->

    <script src="~/Scripts/view/idsrwt/wsd.js"></script> <!--  -->
}
