<div class="container-fluid">
    <div class="alert" id="main-alert"></div>
    <div class="row">
        <div class="col-xl-12 mb-3 mt-2">
            <div class="card border-secondary mb-3">
                <div class="card-header">
                    <div class="row">
                        <div class="col-xl-12 text-center">
                            <div class="container-fluid">
                                <form class="form-inline">
                                    <div class="btn-toolbar" role="toolbar" aria-label="Toolbar with button groups">
                                        <div class="btn-group mr-2" role="group" aria-label="First group">
                                            <button type="button" class="btn btn-outline-secondary" title="Принять состав на станцию" id="arrival">Приб.</button>
                                            <button type="button" class="btn btn-outline-secondary" title="Отправить состав со станции" id="sending">Отпр.</button>
                                            <button type="button" class="btn btn-outline-secondary" title="Дислокация вагонов по станции" id="dislocation">Дисл.</button>
                                            <button type="button" class="btn btn-outline-secondary" title="Роспуск вагонов" id="dissolution">Росп.</button>
                                            <button type="button" class="btn btn-outline-secondary" title="">Выгр.</button>
                                            <button type="button" class="btn btn-outline-secondary" title="">Погр.</button>
                                            <button type="button" class="btn btn-outline-secondary" title="">Весы.</button>
                                            <button type="button" class="btn btn-outline-secondary" title="Предоставить состав для сдачи на дорогу" id="provide">Пред.</button>
                                            <button type="button" class="btn btn-outline-secondary" title="Отправить состав на станцию УЗ" id="sending_uz">Отпр УЗ.</button>
                                        </div>
                                        <div class="btn-group mr-2" role="group" aria-label="Second group">
                                            <button type="button" class="btn btn-outline-secondary" title="">Авто</button>
                                            <button type="button" class="btn btn-outline-secondary" title="">Ручная</button>
                                            <button type="button" class="btn btn-outline-secondary" title="">Реверс</button>
                                        </div>
                                        <div class="btn-group mr-2" role="group" aria-label="Third group">
                                            <div class="input-group w-75">
                                                <input type="text" id="find-num-wagon" class="form-control" placeholder="Номер вагона" aria-label="" aria-describedby="">
                                                <div class="input-group-append">
                                                    <button class="btn btn-outline-secondary" type="button" id="find-wagon">Найти</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-body text-dark pl-3 pr-3">
                    <div class="row">
                        <div class="col-xl-4">
                            <div class="card border-primary">
                                <div class="card-header">
                                    <div id="dds-station"></div>
                                    Дерево путей
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-xl-12">
                                            <div id="tree-way"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xl-8">
                            <div class="card border-info">
                                <div class="card-header">Перечень вагонов</div>
                                <div class="card-body" style="font-size:13px">
                                    <div class="row">
                                        <div class="col-xl-12">
                                            <table id="wagons-way" class="display compact table-striped table-hover" style="width:100%;table-layout: fixed"></table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-footer bg-transparent border-dark"></div>
            </div>
        </div>
    </div>
</div>

<div id="loading_way_detail" title="" style="display:none">
    <form id="form_loading_way_detail">
        <div>
            <ul class="nav nav-tabs" id="myTab" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" id="home-tab" data-toggle="tab" href="#general_loading" role="tab" aria-controls="home" aria-selected="true">Общая</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="profile-tab" data-toggle="tab" href="#operators_loading" role="tab" aria-controls="profile" aria-selected="false">Операторы</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" id="contact-tab" data-toggle="tab" href="#cargo_loading" role="tab" aria-controls="contact" aria-selected="false">Груз</a>
                </li>
            </ul>
            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active" id="general_loading" role="tabpanel" aria-labelledby="home-tab">.</div>
                <div class="tab-pane fade" id="operators_loading" role="tabpanel" aria-labelledby="profile-tab">..</div>
                <div class="tab-pane fade" id="cargo_loading" role="tabpanel" aria-labelledby="contact-tab">...</div>
            </div>
        </div>
        <!-- Allow form submission with keyboard without duplicating the dialog button -->
        <input type="submit" tabindex="-1" style="position:absolute; top:-1000px">
    </form>
</div>

