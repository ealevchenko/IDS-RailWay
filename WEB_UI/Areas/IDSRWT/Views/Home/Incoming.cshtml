<div class="alert" id="incoming-alert"></div>
<div class="row">
    <div class="col-xl-12 mb-3 mt-2">
        <div class="card border-dark mb-3">
            <div class="card-header bg-secondary text-white">Поезда по прибытию</div>
            <div class="card-body text-dark pl-3 pr-3">
                <table class="table table-striped table-bordered" id="table-sostav-arrival" style="width:100%"></table>
            </div>
        </div>
    </div>
</div>

<div id="edit-sostav" title="" style="display:none">
    <div class="alert" id="edit-sostav-alert"></div>
    <form id="form-edit-sostav">
        <div class="form-row">
            <div class="col-md-6 mb-2">
                <label for="train-edit" class="col-form-label">@IDSRWTResource.title_train:</label>
                <input type="text" class="form-control" id="train-edit" placeholder="" name="train-edit" required="required">
                <div class="invalid-feedback">
                    @IDSRWTResource.title_train_invalid
                </div>
            </div>
            <div class="col-md-6 mb-2">
                <label for="composition-index-edit" class="col-form-label">@IDSRWTResource.title_composition_index:</label>
                <input type="text" class="form-control" id="composition-index-edit" placeholder="" name="composition-index-edit" required="required">
                <div class="invalid-feedback">
                    @IDSRWTResource.title_composition_index_invalid
                </div>
            </div>
        </div>
        @*<div class="form-row">

            </div>*@
        <div class="form-row">
            <div class="col-md-6 mb-2">
                <label for="date-arrival-edit" class="col-form-label">@IDSRWTResource.title_date_arrival:</label>
                <input type="datetime" class="form-control" id="date-arrival-edit" placeholder="" name="date-arrival-edit" required="required">
                <div class="invalid-feedback">
                    @IDSRWTResource.title_date_arrival_invalid
                </div>
            </div>
            <div class="col-md-6 mb-2">
                <label for="station-from-edit" class="col-form-label">@IDSRWTResource.title_station_from:</label>
                <select class="form-control" id="station-from-edit" name="station-from-edit" required="required"></select>
                <div class="invalid-feedback">
                    @IDSRWTResource.title_station_from_invalid
                </div>
            </div>
        </div>
        @*<div class="form-row">

            </div>*@
        <div class="form-row">
            <div class="col-md-12 mb-2">
                <label for="note-edit" class="col-form-label">@IDSRWTResource.title_note:</label>
                <textarea class="form-control" id="note-edit" placeholder="" name="note-edit"></textarea>
            </div>
        </div>
        <!-- Allow form submission with keyboard without duplicating the dialog button -->
        <input type="submit" tabindex="-1" style="position:absolute; top:-1000px">
    </form>
</div>

<div id="dialog-confirm" title="Empty the recycle bin?" style="display:none">
    <p id="dialog-confirm-text">
        <span class="ui-icon ui-icon-alert" style="float:left; margin:12px 12px 20px 0;"></span>
    </p>
</div>

<div class="cd-cars-detali">
    <div class="container-fluid">

        <form class="cd-form">
            <div class="card border-secondary">
                <div class="card-header bg-secondary text-white" id="sostav-info">
                    <h1 id="sostav-title"></h1>
                </div>
                <div class="card-body">
                    <div class="alert" id="car-detali-alert"></div>
                    <div class="form-row">
                        <div class="col-md-4 mb-1">
                            <div class="card border-success" id="card-arrived-cars">
